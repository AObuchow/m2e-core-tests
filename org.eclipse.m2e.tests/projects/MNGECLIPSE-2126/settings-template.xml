<settings>
  <localRepository>target/localrepo</localRepository>
  <proxies>
    <proxy>
      <active>true</active>
      <protocol>http</protocol>
      <host>localhost</host>
      <port>@port.http@</port>
      <username>proxyuser</username>
      <password>proxypass</password>
    </proxy>
  </proxies>
  <servers>
    <server>
      <id>testrepo</id>
      <username>testuser</username>
      <password>testpass</password>
    </server>
  </servers>
  <profiles>
    <profile>
      <id>development</id>
      <repositories>
        <repository>
          <id>central</id>
          <url>file:repositories/remoterepo</url>
          <snapshots><enabled>true</enabled></snapshots>
          <releases><enabled>true</enabled></releases>
        </repository>
      </repositories>
      <pluginRepositories>
        <pluginRepository>
          <id>central</id>
          <url>file:repositories/remoterepo</url>
          <snapshots><enabled>true</enabled></snapshots>
          <releases><enabled>true</enabled></releases>
        </pluginRepository>
      </pluginRepositories>
    </profile>
  </profiles>
  <activeProfiles>
    <activeProfile>development</activeProfile>
  </activeProfiles>
</settings>
